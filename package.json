{
	"name": "svelte-piano",
	"version": "0.0.1",
	"private": false,
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": "./package/index.js"
	},
	"svelte": "./package/index.js",
	"types": "./package/index.d.ts",
	"files": [
		"package"
	],
	"scripts": {
		"dev": "vite dev",
		"debug": "NODE_OPTIONS=\"--inspect\" vite dev",
		"build": "vite build",
		"package": "rm -rf package && svelte-kit sync && svelte-package",
		"package-watch": "svelte-package --watch",
		"release": "pnpm package && rm ./package/package.json && changeset publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest"
	},
	"dependencies": {
		"nanostores": "^0.7.1",
		"rxjs": "^7.8.0",
		"svelte": "3.55.0"
	},
	"devDependencies": {
		"@changesets/cli": "2.26.0",
		"@formkit/auto-animate": "1.0.0-beta.5",
		"@neodrag/svelte": "^2.0.3",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/kit": "1.5.0",
		"@sveltejs/package": "1.0.2",
		"@svitejs/changesets-changelog-github-compact": "1.1.0",
		"@tsconfig/svelte": "^3.0.0",
		"@tweakpane/core": "^1.1.4",
		"@tweakpane/plugin-essentials": "^0.1.8",
		"@types/node": "18.13.0",
		"@types/postcss-preset-env": "8.0.0",
		"@types/prismjs": "1.26.0",
		"@typescript-eslint/eslint-plugin": "5.51.0",
		"@typescript-eslint/parser": "5.51.0",
		"autoprefixer": "10.4.13",
		"copy": "0.3.2",
		"cssnano": "5.1.14",
		"dotenv": "16.0.3",
		"eslint": "8.33.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"fracpane": "^0.2.1",
		"fractils": "3.1.0",
		"greset": "2.0.0",
		"lucide-svelte": "^0.112.0",
		"mdsvex": "0.10.6",
		"nodemon": "2.0.20",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.20",
		"postcss-load-config": "4.0.1",
		"postcss-preset-env": "8.0.1",
		"prettier": "2.8.4",
		"prettier-plugin-svelte": "2.9.0",
		"prismjs": "1.29.0",
		"pug": "3.0.2",
		"rehype-autolink-headings": "6.1.1",
		"rehype-slug": "5.1.0",
		"remark-abbr": "1.4.1",
		"sass": "1.58.0",
		"svelte-check": "3.0.3",
		"svelte-copy": "^1.3.0",
		"svelte-preprocess": "5.0.1",
		"svelte2tsx": "0.6.1",
		"tone": "^14.7.77",
		"tslib": "2.5.0",
		"tweakpane": "^3.1.4",
		"typescript": "4.9.5",
		"vite": "4.1.1",
		"vitest": "0.28.4"
	},
	"packageManager": "pnpm@7.0.0",
	"engines": {
		"node": ">= 16",
		"npm": "forbidden, use pnpm",
		"pnpm": ">= 7.0.0"
	}
}